{
  "name": "travel-craft-hub-1-monorepo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "npm@10.9.2",
  "scripts": {
    "dev:frontend": "cd apps/frontend && npm run dev",
    "dev:backend": "cd apps/backend && source venv/bin/activate && python app.py",
    "dev:all": "npm-run-all --parallel dev:frontend dev:backend"
  },
  "dependencies": {
    "firebase": "^12.0.0",
    "react-router-dom": "^7.7.1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
